    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.11.2",
    "react-script": "^2.0.5",
    "react-toastify": "^9.1.3",
    "rrweb": "^2.0.0-alpha.4",
    "rrweb-player": "^1.0.0-alpha.4",
    "tailwind-merge": "^1.12.0",
    "ts-loader": "^9.2.8",
    "typescript": "^5.3.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "xhr-shim": "^0.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "archiver": "^6.0.1",
    "autoprefixer": "^10.4.7",
    "create-file-webpack": "^1.0.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.50.0",
    "postcss": "^8.4.13",
