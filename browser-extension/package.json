{
  "name": "Inssman",
  "version": "1.0.64",
  "description": "Browser extension to Intercept/Modify HTTP Request/Response/Headers/Body",
  "main": "serviceWorker.js",
  "scripts": {
    "dev": "BROWSER=chrome webpack --config webpack/webpack.development.js --watch",
    "build": "npm run build:chrome && npm run build:edge",
    "build:chrome": "BROWSER=chrome webpack --config webpack/webpack.production.js",
    "build:edge": "BROWSER=edge webpack --config webpack/webpack.production.js",
    "clear:dist": "node scripts/removeDir.js",
    "create:zip": "npm run create:zip:chrome && npm run create:zip:edge",
    "create:zip:chrome": "BROWSER=chrome node scripts/createZip.js",
    "create:zip:edge": "BROWSER=edge node scripts/createZip.js",
    "release": "npm run clear:dist && npm run build && npm run create:zip",
    "style": "tailwindcss -i ./src/options/input.css -o ./dist/options/style.css --watch"
  },
  "keywords": [
    "chrome extension",
    "open source",
    "http",
    "proxy",
    "postman",
    "react"
  ],
  "author": "Gevorg Baghdasaryan (vvmgev@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@amplitude/analytics-browser": "^1.9.1",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@mswjs/interceptors": "^0.22.14",
    "@svgr/webpack": "^6.5.1",
    "@types/chrome": "^0.0.287",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "copy-to-clipboard": "^3.3.3",
    "copy-webpack-plugin": "^10.2.4",
    "deepcopy": "^2.1.0",
    "dotenv-webpack": "^8.0.1",
    "fflate": "^0.8.1",
    "file-loader": "^6.2.0",
    "firebase": "^10.7.1",
    "html-webpack-plugin": "^5.5.0",
    "idb": "^8.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mixpanel-browser": "^2.47.0",
    "monaco-editor": "^0.48.0",
    "postcss-preset-env": "^7.5.0",
    "process": "^0.11.10",
    "rc-checkbox": "^3.1.0",
    "rc-dialog": "^9.3.4",
    "rc-input": "^1.4.3",
    "rc-select": "^14.12.0",
    "rc-tooltip": "^6.1.3",
